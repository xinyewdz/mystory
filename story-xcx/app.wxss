/**app.wxss**/
@import '/static/weui.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.list-img{
  width: 50rpx; 
  height: 50rpx;
  margin-right: 16rpx; 
  display: block;
  border-radius: 50%;
}

.avatar{
  border-radius: 50%;
  width:70rpx;
  height:70rpx;
}

.round-click{
  height: 120rpx;
  width: 120rpx;
  background-color: #413939;
  border-radius: 100%;
  position: fixed;
  bottom: 150rpx;
  right: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 9;
}
.round-click .round-bd{
  font-size: 32rpx;
  max-width: 80rpx;
  color: #fff;
  text-align: center;
}

<!--pages/my/story_list.wxml-->
<view class="page">
  <view class="page_bd">
    <view class="weui-media-box weui-media-box_small-appmsg">
      <scroll-view class="weui-cells" scroll-y="true" style="height: 100%;">
        <a wx:for="{{userList}}" class="weui-cell  weui-cell_access" width="100%" id="{{item.id}}" bindtap="userDetail">
          <view class="weui-cell__bd">{{item.name}}</view>
          <view class="weui-cell__ft"></view>
        </a>
      </scroll-view>
    </view>
  </view>
</view>
